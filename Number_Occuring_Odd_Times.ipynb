    "def find_odd_occurances(arr):\n",
    "    \n",
    "    single_val = 0\n",
    "    \n",
    "    for i in arr:  \n",
    "        single_val ^= i    \n",
    "    \n",
    "    return single_val\n",
    "\n",
    "nums = [2, 3, 5, 4, 5, 2, 4, 3, 5, 2, 4, 4, 2]\n",
    "result = find_odd_occurances(nums)       \n",
    "print(result)\n",
    "        \n",
    "# Time = O(n) , Space = O(1)"
   ]
  }
