    "def last_duplicate(arr):\n",
    "    for i in range(len(arr)-1,-1,-1):\n",
    "        if arr[i] == arr[i-1]:\n",
    "            return arr[i],i\n",
    "        \n",
    "\n",
    "nums = [1,1,5,5,6,7]\n",
    "result = last_duplicate(nums)\n",
    "print(result)\n",
    "        "
   ]
